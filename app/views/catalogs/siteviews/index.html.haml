%h1 Listing catalogs_siteviews
= render partial: 'layouts/search',locals: { viewer: 'siteviews' }

%table
  %thead
    %tr
      %th Contract
      %th Visit date
      %th Completed
      %th
      %th
      %th

  %tbody
    - @catalogs_siteviews.each do |catalogs_siteview|
      %tr
        %td= catalogs_siteview.contract_id
        %td= catalogs_siteview.visit_date
        %td= catalogs_siteview.completed
        %td= link_to 'Show', catalogs_siteview
        %td= link_to 'Edit', edit_catalogs_siteview_path(catalogs_siteview)
        %td= link_to 'Destroy', catalogs_siteview, :method => :delete, :data => { :confirm => 'Are you sure?' }

%br
= will_paginate @catalogs_siteviews
= link_to 'New Siteview', new_catalogs_siteview_path
